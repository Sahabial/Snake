<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snakesample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="3"
        android:targetSdkVersion="17" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="awesomeApp"
        android:debuggable="true"
        android:theme="@style/AppTheme" 
        >
        <activity
            android:name="com.example.snakesample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Life Cycle Test"
          android:name="framework.LifeCycleTest"
          android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="One Touch Test"
          android:name="framework.SingleTouchTest"
          android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Multitouch test"
            android:name="framework.MultiTouchTest"
            android:configChanges="keyboard|keyboardHidden|orientation" />
        <activity android:label="Key test"
            android:name="framework.KeyTest"
            android:configChanges="keyboard|keyboardHidden|orientation" />
    </application>

</manifest>
